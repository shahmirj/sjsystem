[user]
    name = Shahmir Javaid
    email = shahmirj@gmail.com
[credential]
    helper = cache --timeout=3600
    username = shahmirj@gmail.com
[color]
    ui = true
    pager = true
    status = true
[merge]
    commit = no
    ff = false
[alias]
    bi = bisect
    br = branch
    brf = branch -f
    ci = commit
    cia = commit --amend
    co = checkout
    cob = checkout -b
    cp = cherry-pick
    di = diff --color --ws-error-highlight=new,old
    fe = fetch
    gr = log --graph --oneline --topo-order --all --decorate --boundary
    gro = log --remotes=origin --graph --oneline --topo-order --decorate --boundary
    me = merge
    mef = merge --ff
    re = rebase
    rea = rebase --abort
    rec = rebase --continue
    rei = rebase --interactive --autosquash
    st = status --short -b
    ta = tag --sort=creatordate
    up = pull --rebase
    sup = submodule update

    search = grep -n
    tree = log --graph --oneline --decorate

[push]
    default = current
[pull]
    default = current
    rebase = true
[fetch]
    prune = true
    prune-tags = true
    tags = true
[filter "media"]
    required = true
    clean = git media clean %f
    smudge = git media smudge %f
[filter "hawser"]
    clean = git hawser clean %f
    smudge = git hawser smudge %f
    required = true
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[diff]
    colorMoved=default
